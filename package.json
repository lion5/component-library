{
  "name": "@lion5/component-library",
  "version": "0.2.13",
  "license": "MIT",
  "contributors": [
    "Michael GÃ¶tz <michael.goetz@lion5.io> (https://github.com/michigg)"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lion5/component-library"
  },
  "keywords": [
    "vue3",
    "vuejs",
    "vite",
    "typescript",
    "ui",
    "library"
  ],
  "scripts": {
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build": "vue-tsc && vite build",
    "test:unit": "vitest --environment jsdom --root src",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/library.umd.js",
  "module": "./dist/library.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/library.es.js",
      "require": "./dist/library.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "dependencies": {
    "vue": ">=3.2.0"
  },
  "peerDependencies": {
    "vue-router": ">=4.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-essentials": "^7.0.0-rc.5",
    "@storybook/addon-interactions": "^7.0.0-rc.5",
    "@storybook/addon-links": "^7.0.0-rc.5",
    "@storybook/blocks": "^7.0.0-rc.5",
    "@storybook/builder-vite": "^7.0.0-rc.5",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/vue3": "^7.0.0-rc.5",
    "@storybook/vue3-vite": "^7.0.0-rc.5",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.15.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/ui": "^0.29.7",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "bootstrap-icons": "^1.10.3",
    "eslint": "^8.36.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.59.3",
    "storybook": "^7.0.0-rc.5",
    "typescript": "~5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.7",
    "vue-tsc": "^1.2.0"
  }
}
