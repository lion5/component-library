{
  "name": "@lion5/component-library",
  "version": "0.1.2",
  "license": "MIT",
  "contributors": [
    "Michael GÃ¶tz <michael.goetz@lion5.io> (https://github.com/michigg)"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lion5/component-library"
  },
  "keywords": [
    "vue3",
    "vuejs",
    "vite",
    "typescript",
    "ui",
    "library"
  ],
  "scripts": {
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build": "vue-tsc && vite build",
    "test:unit": "vitest --environment jsdom --root src",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/library.umd.js",
  "module": "./dist/library.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/library.es.js",
      "require": "./dist/library.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-essentials": "^7.0.0-beta.48",
    "@storybook/addon-interactions": "^7.0.0-beta.48",
    "@storybook/addon-links": "^7.0.0-beta.48",
    "@storybook/blocks": "^7.0.0-beta.48",
    "@storybook/builder-vite": "^7.0.0-beta.48",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/vue3": "^7.0.0-beta.48",
    "@storybook/vue3-vite": "^7.0.0-beta.48",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/ui": "^0.28.5",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "bootstrap-icons": "^1.10.3",
    "eslint": "^8.34.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.58.1",
    "storybook": "^7.0.0-beta.48",
    "typescript": "~4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.0.24"
  }
}
