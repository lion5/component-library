{
  "name": "@lion5/component-library",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "contributors": [
    "Michael GÃ¶tz <michael.goetz@lion5.io> (https://github.com/michigg)"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lion5/component-library"
  },
  "keywords": [
    "vue3",
    "vuejs",
    "vite",
    "typescript",
    "ui",
    "library"
  ],
  "scripts": {
    "lint": "eslint vue-project --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build": "vue-tsc && vite build",
    "test:unit": "vitest --environment jsdom --root src",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/library.umd.js",
  "module": "./dist/library.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/library.es.js",
      "require": "./dist/library.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-essentials": "^7.0.0-beta.21",
    "@storybook/addon-interactions": "^7.0.0-beta.21",
    "@storybook/addon-links": "^7.0.0-beta.21",
    "@storybook/blocks": "^7.0.0-beta.21",
    "@storybook/builder-vite": "^7.0.0-beta.21",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/vue3": "^7.0.0-beta.21",
    "@storybook/vue3-vite": "^7.0.0-beta.21",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.21",
    "typescript": "~4.7.4",
    "vite": "^4.0.4",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
