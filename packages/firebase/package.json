{
  "name": "@lion5/component-library-firebase",
  "version": "0.30.0",
  "type": "commonjs",
  "license": "MIT",
  "contributors": [
    "Michael GÃ¶tz <michael.goetz@lion5.io> (https://github.com/michigg)",
    "Christopher Nagy <christopher.nagy@lion5.io> (https://github.com/nagychris)"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lion5/component-library"
  },
  "keywords": [
    "vue3",
    "vuejs",
    "vite",
    "typescript",
    "ui",
    "library"
  ],
  "scripts": {
    "build": "vite build",
    "test:unit": "cross-env TZ=UTC vitest --config ./vitest.unit.config.ts run",
    "test:unit:ui": "cross-env TZ=UTC vitest --ui --config ./vitest.unit.config.ts",
    "test:storybook": "cross-env TZ=UTC vitest --config ./vitest.config.ts run",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "docs": "storybook dev -p 6006",
    "docs:build": "storybook build -o ../storybook-host/storybook-static/firebase",
    "package-check": "publint",
    "package-type-check": "attw --pack . --exclude-entrypoints ./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./dist/index.css": "./dist/index.css",
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "./dist/index.css"
  ],
  "dependencies": {
    "@lion5/component-library": "0.30.0",
    "firebase": "^11.2.0",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "@lion5/component-library": ">= 0.12.1",
    "firebase": ">= 9.23.0 < 10",
    "vue": ">= 3.4.21"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@chromatic-com/storybook": "^4.0.0",
    "@storybook/addon-a11y": "^9.0.10",
    "@storybook/addon-designs": "^10.0.1",
    "@storybook/addon-docs": "^9.0.10",
    "@storybook/addon-onboarding": "^9.0.10",
    "@storybook/addon-vitest": "^9.0.10",
    "@storybook/vue3-vite": "^9.0.10",
    "@tsconfig/node22": "^22.0.0",
    "@types/dom-screen-wake-lock": "^1.0.3",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "^1.1.25",
    "@vitest/ui": "^3.0.5",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "publint": "^0.3.2",
    "sass-embedded": "^1.83.4",
    "storybook": "^9.0.11",
    "typescript": "~5.7.3",
    "vite": "^6.1.6",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.0"
  },
  "gitHead": "4b5b01f298040c7d4206a67ab76b1b02fe7fe91e",
  "overrides": {
    "storybook": "$storybook"
  }
}
