    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./dist/style.css": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "./dist/style.css"
  ],
