{
  "name": "@lion5/component-library",
  "version": "0.15.3",
  "type": "commonjs",
  "license": "MIT",
  "contributors": [
    "Michael GÃ¶tz <michael.goetz@lion5.io> (https://github.com/michigg)",
    "Christopher Nagy <christopher.nagy@lion5.io> (https://github.com/nagychris)"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lion5/component-library"
  },
  "keywords": [
    "vue3",
    "vuejs",
    "vite",
    "typescript",
    "ui",
    "library"
  ],
  "scripts": {
    "build": "vite build",
    "test:unit": "cross-env TZ=UTC vitest run",
    "test:unit:ui": "cross-env TZ=UTC vitest --ui",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "docs": "storybook dev -p 6006",
    "docs:build": "storybook build -o ../storybook-host/storybook-static/core",
    "package-check": "publint",
    "package-type-check": "attw --pack . --exclude-entrypoints ./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./dist/style.css": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "./dist/style.css"
  ],
  "dependencies": {
    "bootstrap-icons": ">=1.11.3",
    "lodash-es": ">=4.17.21",
    "pinia": ">=2.1.7",
    "remove": "^0.1.5",
    "vee-validate": ">=4.12.6",
    "vue": ">=3.4.21",
    "vue-multiselect": ">=3.0.0",
    "vue-router": ">=4.3.0",
    "yup": ">=1.4.0"
  },
  "peerDependencies": {
    "bootstrap-icons": ">=1.11.3",
    "lodash-es": ">=4.17.21",
    "pinia": ">=2.1.7",
    "vee-validate": ">=4.12.6",
    "vue": ">=3.4.21",
    "vue-router": ">=4.3.0",
    "yup": ">=1.4.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@storybook/addon-a11y": "^8.0.8",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/blocks": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@storybook/vue3": "^8.0.8",
    "@storybook/vue3-vite": "^8.0.8",
    "@types/dom-screen-wake-lock": "^1.0.3",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^1.5.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint-plugin-storybook": "^0.8.0",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "publint": "^0.2.7",
    "sass": "^1.75.0",
    "storybook": "^8.0.8",
    "typescript": "^5.4.5",
    "vite": "^5.2.14",
    "vite-plugin-dts": "^3.8.2",
    "vitest": "^1.5.0",
    "vue-tsc": "^2.0.13"
  },
