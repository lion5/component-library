{
  "name": "@lion5/component-library",
  "version": "0.8.3",
  "license": "MIT",
  "contributors": [
    "Michael GÃ¶tz <michael.goetz@lion5.io> (https://github.com/michigg)",
    "Christopher Nagy <christopher.nagy@lion5.io> (https://github.com/nagychris)"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lion5/component-library"
  },
  "keywords": [
    "vue3",
    "vuejs",
    "vite",
    "typescript",
    "ui",
    "library"
  ],
  "scripts": {
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build": "vite build",
    "test:unit": "cross-env TZ=UTC vitest --environment jsdom --root src",
    "test:unit:ui": "cross-env TZ=UTC vitest --environment jsdom --root src --ui",
    "type-check": "vue-tsc -p tsconfig.json --composite false"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/library.cjs.js",
  "module": "./dist/library.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/library.es.js",
      "require": "./dist/library.cjs.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vue-multiselect": "^3.0.0-beta.2",
    "vue-router": ">=4.2.1",
    "pinia": ">=2.1.3",
    "vee-validate": ">=4.9.5",
    "yup": ">=1.1.1",
    "@lion5/component-library-shared": "*"
  },
  "devDependencies": {
    "vite-plugin-dts": "^2.3.0",
    "@rushstack/eslint-patch": "^1.3.0",
    "@types/dom-screen-wake-lock": "^1.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.14",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/ui": "^0.31.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.14.0",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "typescript": "~4.9.5",
    "vite": "^4.3.8",
    "vitest": "^0.31.1",
    "vue-tsc": "^1.7.8"
  }
}
